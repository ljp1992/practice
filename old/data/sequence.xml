<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="task_order" model="ir.sequence">
            <field name="name">task_order</field>
            <field name="code">task.code</field>
            <field name="prefix">T%(year)s%(month)s%(day)s</field>
            <field name="padding">3</field>
        </record>

        <record id="sale_order_sequence" model="ir.sequence">
            <field name="name">sale_order_sequence</field>
            <field name="code">sale.order.code</field>
            <field name="prefix">SO%(year)s%(month)s%(day)s</field>
            <field name="padding">3</field>
        </record>

        <record id="purchase_order_sequence" model="ir.sequence">
            <field name="name">purchase_order_sequence</field>
            <field name="code">purchase.order.code</field>
            <field name="prefix">PO%(year)s%(month)s%(day)s</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>